var disablePagination=!1;$((function(){if($(".privacy-btn").click((function(t){var i=$(this).attr("input_key"),e=$(this).attr("input_value"),a={};a[i]=e,$.ajax({type:"POST",url:"/user/security/privacy",data:a,async:!0,dataType:"html"})})),$(".clipboard textarea").focus((function(){var t=$(this);t.select(),t.mouseup((function(){return t.unbind("mouseup"),!1}))})),"undefined"!=typeof tooltips&&$(window).width()>1024&&tooltips(),$(window).width()>1024&&$(".column-near-sidebar").length>0){var t=$(".backend-sidebar"),i=$(".backend-sidebar .sidebar_inner"),e=t.width(),a=i.height(),o=$(".column-near-sidebar").offset().top,n=t.offset()&&t.offset().top-20;i.css("width",e),$(window).scroll((function(){var t=$(this).scrollTop(),e=$(".column-near-sidebar").height();t>n&&t<o+e-a?i.css({top:"20px",bottom:"auto",position:"fixed"}):t>=o+e-a?i.css({top:"auto",bottom:"0px",position:"absolute"}):i.css("position","static")}))}$(".page_knowledge-base .backend-sidebar .sm_body li a").click((function(t){t.preventDefault();var i=$(this).attr("href");scrollToElement(i,1500,-20)})),$(".dropdown-menu input, .dropdown-menu label").click((function(t){t.stopPropagation()})),$("#tabs").length>0&&($("#tabs").tab({collapsible:!0,active:!1}),$(".tab_close").click((function(){$("#tabs li").removeClass("active"),$(".mail-provider .tab-pane").removeClass("active")}))),$(".alert-inline .close-alert").click((function(){$(this).closest(".alert-inline").fadeOut(300)}))}));